#
# Parameters
CACHEDIR /haarp/
PLUGINSDIR /etc/haarp/plugins/
# Limit in percent (%)
CACHE_LIMIT 95

#Zero Penalty Hit for haarp
# defects:
# ZPH_TOS_LOCAL 0
# recomended:
# ZPH_TOS_LOCAL 8

#
#  MySQL Configuration
MYSQL_HOST localhost
MYSQL_USER root
MYSQL_PASS haarpcache
MYSQL_DB haarp

# extentions
JPG_MIN 1000
JPG_MAX 0
JPG_EXP 86400

JPEG_MIN 1000
JPEG_MAX 0
JPEG_EXP 86400

GIF_MIN 1000
GIF_MAX 0
GIF_EXP 86400

FLV_MIN 1000
FLV_MAX 0
FLV_EXP 86400

WMV_MIN 1000
WMV_MAX 0
WMV_EXP 86400

WMA_MIN 1000
WMA_MAX 0
WMA_EXP 86400

RMVB_MIN 1000
RMVB_MAX 0
RMVB_EXP 86400

MPG_MIN 1000
MPG_MAX 0
MPG_EXP 86400

MPEG_MIN 1000
MPEG_MAX 0
MPEG_EXP 86400

AVI_MIN 1000
AVI_MAX 0
AVI_EXP 86400

SWF_MIN 1000
SWF_MAX 0
SWF_EXP 86400

DOC_MIN 1000
DOC_MAX 0
DOC_EXP 86400

DOCX_MIN 1000
DOCX_MAX 0
DOCX_EXP 86400

ZIP_MIN 1000
ZIP_MAX 0
ZIP_EXP 86400

RAR_MIN 1000
RAR_MAX 0
RAR_EXP 86400

EXE_MIN 1000
EXE_MAX 0
EXE_EXP 86400

PPT_MIN 1000
PPT_MAX 0
PPT_EXP 86400

PDF_MIN 1000
PDF_MAX 0
PDF_EXP 86400

ORKUT_NORESUME true
ORKUT_NODOWN true
#
# For security reasons its best not to run as root
#
# Default:
# USER root
# GROUP root

#
# Run as a daemon service
# Set false to run in foreground
#
# Default:
# Padr√£o:
# DAEMON true

#
# pidfile location
# Userd by Haarp and init.d
#
# Default:
PIDFILE @localstatedir@/log/haarp/haarp.pid

#
# Number of child processes
# initial value SERVERNUMBER
# Maximum value MAXSERVERS
#
# Default:
SERVERNUMBER 250
MAXSERVERS 1000
 
# Logging
#
# Default:
# ACCESSLOG @localstatedir@/log/haarp/access.log
# ERRORLOG @localstatedir@/log/haarp/haarp.log

#
# Log levels
#  0 = ERROR
#  1 = VERBOSE
#
# Default:
LOGLEVEL 0

#
# Run as a transparent proxy
#
# Default:
# TRANSPARENT false

#
# Parent Proxy
#
# Default:
# Standar: NONE
# PARENTPROXY localhost
# PARENTPORT 3128

# Write a users real IP addres in logs
#
# Default:
# FORWARDED_IP false

#
# Enable X-Forwarded-For headers
#
#
# Default:
# X_FORWARDED_FOR false

#
# Proxy listening port
#
# Default:
PORT 8123

#
# Proxy listening address
#
# Default:
# BIND_ADDRESS 127.0.0.1

